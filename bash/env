##
# DELUXE-USR-LOCAL-BIN-INSERT
# (do not remove this comment)
##
echo $PATH | grep -q -s "/usr/local/bin"
if [ $? -eq 1 ] ; then
  PATH=$PATH:/usr/local/bin
  export PATH
fi

export PATH=~/bin:$PATH

export PATH=/opt/local/bin:$PATH
export PATH=$PATH:/opt/railsbench/bin
export PATH=$PATH:/opt/jruby/bin

export PATH=$PATH:/Developer/SDKs/AIR
export PATH=$PATH:/Developer/SDKs/Flex
export PATH=$PATH:/Developer/SDKs/Android/Current/tools

export MANPATH=$MANPATH:/opt/local/share/man/
export EDITOR=vim
export LANG=C

# wl
export WLDOMAIN=steffenleistner
export CONNECT=~/Projects/Work/2008/connect

# vim:syntax=sh


